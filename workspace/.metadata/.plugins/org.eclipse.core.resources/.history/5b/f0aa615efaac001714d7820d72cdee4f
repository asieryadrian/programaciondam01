package cuarto;

/**
 * @author usuario
 *
 */
public class cuarto {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int contador = 0;
		int a = 1;
		while (contador <= 100) {
			a= a + contador;
			contador ++;
		} 
		System.out.println(a);	
	}

}
